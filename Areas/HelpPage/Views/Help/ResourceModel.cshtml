@using System.Web.Http
@using OpenGovApi.Areas.HelpPage.ModelDescriptions
@model ModelDescription
@{
    ViewBag.Title = Model.Name;
}
<div class="container">
    <div class="row">
        <div class="col-xs-12">
            <header class="page-header">
                <h1>@ViewBag.Title</h1>
            </header>
            <p>@Model.Documentation</p>
            <section>
                @Html.DisplayFor(m => Model)
            </section>
        </div>
    </div>
</div>
